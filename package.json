{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "test coverage for [babel-plugin-lodash (v3.2.11)](https://github.com/lodash/babel-plugin-lodash#readme) [![npm package](https://img.shields.io/npm/v/npmtest-babel-plugin-lodash.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-babel-plugin-lodash) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-babel-plugin-lodash.svg)](https://travis-ci.org/npmtest/node-npmtest-babel-plugin-lodash)",
    "devDependencies": {
        "babel-plugin-lodash": "*",
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-babel-plugin-lodash",
    "keywords": [
        "coverage",
        "test",
        "babel-plugin-lodash"
    ],
    "license": "MIT",
    "main": "lib.npmtest_babel_plugin_lodash.js",
    "name": "npmtest-babel-plugin-lodash",
    "nameAlias": "npmtest_babel_plugin_lodash",
    "nameOriginal": "npmtest-babel-plugin-lodash",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-babel-plugin-lodash.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
